[32m+[m
[32m+[m[32m<?php[m
[32m+[m[32m$js = <<<OEF[m
[32m+[m[32m//get passport details of the person who giving claim[m
[32m+[m[32m$("#get-data-button").on('click', function () {[m
[32m+[m[32m    getPassportData();[m
[32m+[m[32m});[m
[32m+[m[32mfunction getPassportData() {[m
[32m+[m
[32m+[m[32m    var pinfl = $("#docconscript-pinfl").val();[m
[32m+[m[32m    var passSeria = $("#docconscript-passport_seria").val();[m
[32m+[m[32m    var passNumber = $("#docconscript-passport_number").val();[m
[32m+[m[32m    var passport = passSeria + passNumber;[m
[32m+[m[32m    $.ajax({[m
[32m+[m[32m        url: "/doc-conscript/person",[m
[32m+[m[32m        data: {[m
[32m+[m[32m        "pinfl": pinfl,[m
[32m+[m[32m            "passport": passport[m
[32m+[m[32m        },[m
[32m+[m[32m        dataType: "json",[m
[32m+[m[32m        type: "GET",[m
[32m+[m[32m        timeout: 10000,[m
[32m+[m[32m        //beforeSend: function (xhr) {[m
[32m+[m[32m        // xhr.setRequestHeader('Authorization', 'Bearer ' + access_token);[m
[32m+[m[32m        //$('.loader').fadeIn();[m
[32m+[m
[32m+[m[32m    //},[m
[32m+[m[32m        success: function (data) {[m
[32m+[m[32m        //$('.loader').fadeOut();[m
[32m+[m[32m        if (data != "") {[m
[32m+[m[32m            $("#docconscript-first_name").val(data.name_latin);[m
[32m+[m[32m            $("#docconscript-last_name").val(data.surname_latin);[m
[32m+[m[32m            $("#docconscript-patronymic").val(data.patronym_latin);[m
[32m+[m[32m            $("#docconscript-passport_given_date").val(data.date_begin_document);[m
[32m+[m[32m            //$("#docinsuranceform-passvaliditydate").val(data.date_end_document);[m
[32m+[m[32m            $("#docconscript-passport_issued_by").val(data.doc_give_place);[m
[32m+[m[32m            //$("#docinsuranceform-genderid").val(data.sex);[m
[32m+[m[32m            $("#docconscript-birth_date").val(data.birth_date);[m
[32m+[m[32m            //alert('Данные успешно получены');[m
[32m+[m[32m        } else {[m
[32m+[m[32m            alert('Произошла ошибка при получении данных. Проверьте данные!');[m
[32m+[m[32m        }[m
[32m+[m[32m    },[m
[32m+[m[32m        error: function () {[m
[32m+[m[32m        //$('.loader').fadeOut();[m
[32m+[m[32m        alert('Произошла ошибка при получении данных. Проверьте данные!');[m
[32m+[m[32m    }[m
