\ No newline at end of file[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mnamespace app\components;[m
[32m+[m
[32m+[m[32muse DateTime;[m
[32m+[m[32muse SimpleXMLElement;[m
[32m+[m[32muse SoapClient;[m
[32m+[m
[32m+[m[32mclass PassportService[m
[32m+[m[32m{[m
[32m+[m
[32m+[m[32m    public static function getData($pinfl, $passport)[m
[32m+[m[32m    {[m
[32m+[m[32m        $wsdl = "https://ips.gov.uz/mediate/ips/PC/PersonDocInfoService?wsdl";[m
[32m+[m
[32m+[m
[32m+[m[32m        $client = new SoapClient([m
[32m+[m[32m            $wsdl, [[m
[32m+[m[32m                'cache_wsdl' => WSDL_CACHE_NONE,[m
[32m+[m[32m            ][m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $xml = "";[m
[32m+[m
[32m+[m[32m        self::formRequestStringForPassport($xml, $pinfl, $passport);[m
[32m+[m[32m        $soapData = $client->soapMsg_Receive(['Data' => $xml]);[m
[32m+[m
[32m+[m[32m        if ($soapData->Result != "1") {[m
[32m+[m[32m            return [];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        $data = new SimpleXMLElement($soapData->Data);[m
[32m+[m[32m        $data = (array)$data->row;[m
[32m+[m[32m        $data['birth_date'] = isset($data['birth_date']) ? date("Y-m-d", strtotime($data['birth_date'])) : "";[m
[32m+[m
[32m+[m[32m        $data['date_begin_document'] = (array)DateTime::createFromFormat("Y-d-m", $data['date_begin_document']);[m
[32m+[m[32m        $data['date_begin_document'] = $data['date_begin_document']['date'];[m
[32m+[m[32m        $data['date_begin_document'] = date("Y-m-d", strtotime($data['date_begin_document']));[m
[32m+[m
[32m+[m[32m        $data['date_end_document'] = (array)DateTime::createFromFormat("Y-d-m", $data['date_end_document']);[m
[32m+[m[32m        $data['date_end_document'] = $data['date_end_document']['date'];[m
[32m+[m[32m        $data['date_end_document'] = date("Y-m-d", strtotime($data['date_end_document']));[m
[32m+[m[32m        return $data;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function transliterate($textcyr = null, $textlat = null)[m
[32m+[m[32m    {[m
[32m+[m[32m        $cyr = array([m
[32m+[m[32m            'ж', 'ч', 'щ', 'ш', 'ю', 'а', 'б', 'в', 'г', 'д', 'е', 'з', 'и', 'ж', 'к', 'л', 'м', 'н', 'у', 'о', 'п', 'р', 'с', 'т', 'у', 'ф', 'х', 'ц', 'ъ', 'х', 'к',[m
[32m+[m[32m            'Ж', 'Ч', 'Щ', 'Ш', 'Ю', 'А', 'Б', 'В', 'Г', 'Д', 'Е', 'З', 'И', 'Ж', 'К', 'Л', 'М', 'Н', 'У', 'О', 'П', 'Р', 'С', 'Т', 'У', 'Ф', 'Х', 'Ц', 'Ъ', 'Х', 'К');[m
[32m+[m[32m        $lat = array([m
[32m+[m[32m            'zh', 'ch', 'sht', 'sh', 'yu', 'a', 'b', 'v', 'g', 'd', 'e', 'z', 'i', 'j', 'k', 'l', 'm', 'n', 'o‘', 'o', 'p', 'r', 's', 't', 'u', 'f', 'h', 'c', 'y', 'x', 'q',[m
[32m+[m[32m            'Zh', 'Ch', 'Sht', 'Sh', 'Yu', 'A', 'B', 'V', 'G', 'D', 'E', 'Z', 'I', 'J', 'K', 'L', 'M', 'N', 'O‘', 'O', 'P', 'R', 'S', 'T', 'U', 'F', 'H', 'c', 'Y', 'X', 'Q');[m
[32m+[m[32m        if ($textcyr) return str_replace($cyr, $lat, $textcyr);[m
[32m+[m[32m        else if ($textlat) return str_replace($lat, $cyr, $textlat);[m
[32m+[m[32m        else return null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private static function formRequestStringForPassport(&$xml, $pinfl, $passport)[m
[32m+[m[32m    {[m
[32m+[m[32m        $xml = <<<XML[m
[32m+[m[32m<?xml version="1.0" encoding="utf-8"?>[m
[32m+[m[32m<DataCEPRequest>[m
[32m+[m[32m     <pinpp>$pinfl</pinpp>[m
[32m+[m[32m     <document>$passport</document>[m
[32m+[m[32m     <langId>1</langId>[m
[32m+[m[32m</DataCEPRequest>[m
[32m+[m[32mXML;[m
[32m+[m[32m    }[m
